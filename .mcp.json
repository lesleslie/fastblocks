{
  "mcpServers": {
    "cloud-run": {
      "args": [
        "-y",
        "https://github.com/GoogleCloudPlatform/cloud-run-mcp"
      ],
      "command": "npx",
      "type": "stdio"
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp@1.0.20"
      ],
      "command": "npx",
      "type": "stdio"
    },
    "crackerjack": {
      "type": "http",
      "url": "http://localhost:8676/mcp"
    },
    "excalidraw": {
      "type": "http",
      "url": "http://localhost:3032/mcp"
    },
    "logfire": {
      "args": [
        "logfire-mcp@latest"
      ],
      "command": "uvx",
      "type": "stdio"
    },
    "macos_automator": {
      "args": [
        "-y",
        "@steipete/macos-automator-mcp@0.4.1"
      ],
      "command": "npx",
      "type": "stdio"
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory@2025.9.25"
      ],
      "command": "npx",
      "type": "stdio"
    },
    "mermaid": {
      "type": "http",
      "url": "http://localhost:3033/mcp"
    },
    "peekaboo": {
      "args": [
        "-y",
        "@steipete/peekaboo-mcp@2.0.3"
      ],
      "command": "npx",
      "env": {
        "PEEKABOO_AI_PROVIDERS": "anthropic/claude-opus-4,anthropic/claude-sonnet-4"
      },
      "type": "stdio"
    },
    "penpot": {
      "args": [
        "penpot-mcp"
      ],
      "command": "uvx",
      "type": "stdio"
    },
    "playwright": {
      "args": [
        "@playwright/mcp@latest"
      ],
      "command": "npx",
      "type": "stdio"
    },
    "sentry": {
      "type": "http",
      "url": "https://mcp.sentry.dev/mcp"
    },
    "session-buddy": {
      "type": "http",
      "url": "http://localhost:8678/mcp"
    },
    "turso-cloud": {
      "args": [
        "-y",
        "mcp-turso-cloud"
      ],
      "command": "npx",
      "type": "stdio"
    },
    "upstash": {
      "args": [
        "-y",
        "@upstash/mcp-server",
        "run"
      ],
      "command": "npx",
      "type": "stdio"
    }
  }
}
